{
    "version": 2,
    "builds": [
      {
        "src": "./src/app.ts",
        "name": "v3",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "./src/routers/api.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  