{
    "version": 2,
    "builds": [
        {
            "src": "./serverless.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/serverless.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}