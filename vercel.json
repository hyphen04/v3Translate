{
    "builds": [
        {
            "src": "api/serverless.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/serverless.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/public/$1"
        }
    ]
}
