{
    "version": 2,
    "name": "yatrazen-translate",
    "builds": [
       { "src": "./src/app.ts", "use": "@vercel/node" }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/index.ts"
        },
        {
            "src" : "/api/(.*)",
            "dest" : "/api.ts"
        }
    ]
 }